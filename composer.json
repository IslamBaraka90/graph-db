{
    "name": "islambaraka90/graph-db",
    "description": "a firndly interface to use many graph db for php applications",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Islambaraka90\\GraphDb\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Islam Baraka",
            "email": "islam.baraka.90@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "vlucas/phpdotenv": "^5.5"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.6",
        "laudis/neo4j-php-client": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
